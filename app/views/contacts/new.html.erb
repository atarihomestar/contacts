<%= form_for @contact do |f| %>

<div class="">
  <%= f.label :name %>:
  <%= f.text_field :name %>
</div>


<div class="">
  <%= f.label :email %>:
  <%= f.text_field :email %>
</div>

<div class="">
  <%= f.label :phone_number %>:
  <%= f.text_field :phone_number %>
</div>

<div class="">
  <%= f.label :address %>:
  <%= f.text_field :address %>
</div>

  <%= f.submit %>

<% end %>
